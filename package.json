{
    "name": "fricolab-website",
    "version": "2.0.0",
    "description": "Fricolab corporate website built with Eleventy",
    "scripts": {
        "start": "eleventy --serve",
        "build": "eleventy",
        "clean": "rimraf _site",
        "predeploy": "npm run clean && npm run build && touch _site/.nojekyll",
        "deploy": "gh-pages -d _site -t",
        "serve": "serve ./_site/"
    },
    "keywords": [
        "eleventy",
        "fricolab",
        "cloud",
        "kubernetes"
    ],
    "author": "Fricolab",
    "license": "ISC",
    "devDependencies": {
        "@11ty/eleventy": "^2.0.1",
        "gh-pages": "^6.3.0",
        "rimraf": "^6.0.1",
        "serve": "^14.2.5"
    }
}
